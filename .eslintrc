{
  "rules": {
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": ["error", { "argsIgnorePattern": "next|Sequelize" }],
    "no-use-before-define": ["error", { "variables": false }],
    "no-multi-str": 0,
    "arrow-body-style": 0,
    "no-empty": ["error", { "allowEmptyCatch": true }],
    "func-names": "off"
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 9
  },
  "extends": [
    "airbnb-base"
  ]
}
